{
  "x-tyk-api-gateway": {
    "info": {
      "id": "api-bridge-agent-id",
      "name": "API Bridge Agent",
      "state": {
        "active": true
      }
    },
    "upstream": {
      "url": ""
    },
    "server": {
      "listenPath": {
        "value": "/api-bridge-agent/",
        "strip": false
      }
    },
    "middleware": {
      "global": {
        "pluginConfig": {
          "data": {
            "enabled": true,
            "value": {
              "mcpServers": {
                "ddg": {
                  "command": "uvx",
                  "args": ["duckduckgo-mcp-server"]
                }
              }
            }
          },
          "driver": "goplugin"
        },
        "prePlugins": [
          {
            "enabled": true,
            "functionName": "APIBridgeAgent",
            "path": "middleware/agent-bridge-plugin.so"
          }
        ]
      }
    }
  },
  "openapi": "3.0.1",
  "info": {
    "title": "API Bridge Agent",
    "description": "API Bridge Agent services",
    "version": "0.0.1"
  },
  "servers": [
    {
      "url": "/"
    }
  ],
  "paths": {
    "/info": {
      "get": {
        "summary": "API Bridge Agent Information",
        "responses": {
          "200": {
            "description": "",
            "content": {}
          }
        }
      }
    },
    "/nlq": {
      "post": {
        "summary": "Ask a natural language query question. Will use OpenAPI services or MCP servers",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "NLQ Response.",
            "content": {}
          }
        }
      }
    },
    "/openapis": {
      "post": {
        "summary": "Ask a natural language query question, only use configured OpenAPI services",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "NLQ Response.",
            "content": {}
          }
        }
      }
    },
    "/mcp/init": {
      "post": {
        "summary": "Initialize MCP servers",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "MCP initialization state.",
            "content": {}
          }
        }
      }
    },
    "/mcp": {
      "post": {
        "summary": "Call an MCP server from a natural language query",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "NLQ Response.",
            "content": {}
          }
        }
      }
    }
  },
  "components": {}
}
